{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FilesystemStorage": {
    "DataDirectory": "/tmp/lamina"
  },
  "AllowedHosts": "*",
  "StorageType": "InMemory",
  "MultipartUploadCleanup": {
    "Enabled": false,
    "CleanupIntervalMinutes": 60,
    "UploadTimeoutHours": 24
  },
  "Authentication": {
    "Enabled": false
  }
}