{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "StorageType": "Filesystem",
  "FilesystemStorage": {
    "DataDirectory": "/tmp/lamina-directory/data",
    "MetadataDirectory": "/tmp/lamina-directory/metadata",
    "MetadataMode": "SeparateDirectory"
  },
  "BucketDefaults": {
    "Type": "Directory",
    "StorageClass": "EXPRESS_ONEZONE",
    "Region": "us-west-2"
  },
  "Authentication": {
    "Enabled": false
  },
  "AutoBucketCreation": {
    "Enabled": false
  }
}