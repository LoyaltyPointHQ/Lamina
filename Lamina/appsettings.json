{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*", 
  "StorageType": "Filesystem",
  "FilesystemStorage": {
    "DataDirectory": "/data",
    "MetadataMode": "Inline"
  },
  "MultipartUploadCleanup": {
    "Enabled": true,
    "CleanupIntervalMinutes": 60,
    "UploadTimeoutHours": 24
  },
  "Authentication": {
    "Enabled": false,
    "Users": []
  }
}
